        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.prashantmn17.myapp"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7a67550f-dc71-432e-9690-6fa1573428f2"
      }
    },
    "plugins": [
      "expo-asset"
    ]
  }
